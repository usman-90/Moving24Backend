{
    "version":2,
    "builds" : [
        {
            "src": "src/main.ts",
            "use": "@now/node-server"
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ]
}
